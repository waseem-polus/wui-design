{
  "name": "@willoui/extras",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "author": "Waseem Polus (https://github.com/waseem-polus)",
  "files": [
    "dist"
  ],
  "main": "./dist/wui-extras.umd.cjs",
  "module": "./dist/wui-extras.js",
  "exports": {
    ".": {
      "import": "./dist/wui-extras.js",
      "require": "./dist/wui-extras.umd.cjs"
    }
  },
  "scripts": {
